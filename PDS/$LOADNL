//            IUNT=563,OUNT=SYSALLDA,IVL=WYLTSO,OVL=WORKE3,DSP2=CATLG,
//            DSP1=NEW,DSP3='',P=30,S=60,D=224,QUAL=WYLTSO
//DOLOAD  EXEC PGM=IEBCOPY,REGION=6M
//SYSPRINT DD SYSOUT=*
//SYSUT1 DD DISP=SHR,DSN=&IPRE..&DASN,UNIT=&IUNT,
//       VOL=(,RETAIN,SER=&IVL),LABEL=(&LBL,&XL,EXPDT=98000),
//       FREE=CLOSE
//SYSUT2 DD UNIT=&OUNT,VOL=SER=&OVL,DSN=&OPRE..&QUAL..&DASN,
//       DISP=(&DSP1,&DSP2&DSP3),SPACE=(TRK,(&P,&S,&D),RLSE)
//SYSIN DD DUMMY
//  PEND
//*
