&----------------------------  LIBRARY UTILITY  --------------------------------
&SELECT OPTION ===>%CODE                                                       $
&
&   C$- COMPRESS DATASET                   &P$- PRINT MEMBER
&   X$- PRINT INDEX LISTING                &R$- RENAME MEMBER
&   L$- PRINT ENTIRE DATASET               &D$- DELETE MEMBER
&   BLANK$- DISPLAY MEMBER LIST            &B$- BROWSE MEMBER
$
$SPF LIBRARY DATASET:
$   PROJECT&===>%PROJECT $
$   LIBRARY&===>%LIBRARY $
$   TYPE   &===>%TYPE    $
$   MEMBER &===>%MEMBER  $ (IF OPTION "P", "R", "D", OR "B" SELECTED)
$   NEWNAME&===>%NEWNAME $ (IF OPTION "R" SELECTED)
$
$OTHER PARTITIONED OR SEQUENTIAL DATASET:
$   DATASET NAME &===>%DSN                                                     $
$   VOLUME SERIAL&===>%VOL   $  (IF NOT CATALOGED)
$
$DATASET PASSWORD&===>¬PSWD    $(IF PASSWORD PROTECTED)
<ACTION>
 1 CODE    CHAR(1)  INIT($PARM1) CURSOR   GENHELP(TUL)
           LIST  (C,L,X,P,R,D,B,' ',*)
           RETURN(Z,L,X,P,R,D,B,M  ,?);
 2 PROJECT CHAR(8)  INIT($PARM2) NOCURSOR;
 3 LIBRARY CHAR(8)  INIT($PARM3) NOCURSOR;
 4 TYPE    CHAR(8)  INIT($PARM4) NOCURSOR;
 5 DSN     CHAR(56) INIT($PARM5) NOCURSOR;
 6 VOL     CHAR(6)  INIT($PARM6) NOCURSOR;
 7 PSWD    CHAR(8)  INIT($PARM7) NOCURSOR;
 8 MEMBER  CHAR(8)  INIT($PARM8) NOCURSOR;
 9 NEWNAME CHAR(8)  INIT($PARM9) NOCURSOR;
10 *       CHAR(8)  INIT('SPFCOPY '); - COMPRESS PGM NAME IF NOT IEBCOPY
11 *       CHAR(8)  INIT('PPRINTED'); - PRINT MEMBER OPT/RESPONSE
12 *       CHAR(8)  INIT('RRENAMED'); - RENAME MEMBER OPT/RESPONSE
13 *       CHAR(8)  INIT('DDELETED'); - DELETE MEMBER OPT/RESPONSE
14 *       CHAR(8)  INIT('B       '); - BROWSE MEMBER OPT/RESPONSE
<END>
