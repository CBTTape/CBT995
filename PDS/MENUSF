2 SELECT CHAR(8) LIST(   1   ,   2   ,   3   )
               RETURN(VT$AR  ,VT$BK  ,VT$SM  );
3 * CHAR(8) INIT('       ');
4 * CHAR(8) INIT('        ');
5 * CHAR(8) INIT('VT$AR   ');
<END>
