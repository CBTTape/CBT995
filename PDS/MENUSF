./ ADD NAME=UDA1     0100-80038-80038-2341-00038-00038-00000-$SYS13  00
&----------------------------  LIBRARY UTILITY  --------------------------------
&SELECT OPTION ===>%CODE                                                       $
&
&   C$- COMPRESS DATASET                   &P$- PRINT MEMBER
&   X$- PRINT INDEX LISTING                &R$- RENAME MEMBER
&   L$- PRINT ENTIRE DATASET               &D$- DELETE MEMBER
&   BLANK$- DISPLAY MEMBER LIST            &B$- BROWSE MEMBER
$
$SPF LIBRARY DATASET:
$   PROJECT&===>%PROJECT $
$   LIBRARY&===>%LIBRARY $
$   TYPE   &===>%TYPE    $
$   MEMBER &===>%MEMBER  $ (IF OPTION "P", "R", "D", OR "B" SELECTED)
$   NEWNAME&===>%NEWNAME $ (IF OPTION "R" SELECTED)
$
$OTHER PARTITIONED OR SEQUENTIAL DATASET:
$   DATASET NAME &===>%DSN                                                     $
$   VOLUME SERIAL&===>%VOL   $  (IF NOT CATALOGED)
$
$DATASET PASSWORD&===>¬PSWD    $(IF PASSWORD PROTECTED)
<ACTION>
 1 CODE    CHAR(1)  INIT($PARM1) CURSOR   GENHELP(TUL)
           LIST  (C,L,X,P,R,D,B,' ',*)
           RETURN(Z,L,X,P,R,D,B,M  ,?);
 2 PROJECT CHAR(8)  INIT($PARM2) NOCURSOR;
 3 LIBRARY CHAR(8)  INIT($PARM3) NOCURSOR;
 4 TYPE    CHAR(8)  INIT($PARM4) NOCURSOR;
 5 DSN     CHAR(56) INIT($PARM5) NOCURSOR;
 6 VOL     CHAR(6)  INIT($PARM6) NOCURSOR;
 7 PSWD    CHAR(8)  INIT($PARM7) NOCURSOR;
 8 MEMBER  CHAR(8)  INIT($PARM8) NOCURSOR;
 9 NEWNAME CHAR(8)  INIT($PARM9) NOCURSOR;
10 *       CHAR(8)  INIT('SPFCOPY '); - COMPRESS PGM NAME IF NOT IEBCOPY
11 *       CHAR(8)  INIT('PPRINTED'); - PRINT MEMBER OPT/RESPONSE
12 *       CHAR(8)  INIT('RRENAMED'); - RENAME MEMBER OPT/RESPONSE
13 *       CHAR(8)  INIT('DDELETED'); - DELETE MEMBER OPT/RESPONSE
14 *       CHAR(8)  INIT('B       '); - BROWSE MEMBER OPT/RESPONSE
<END>
./ ADD NAME=V$AR     0120-80014-80039-0852-00062-00061-00000-TYD9801 00
&------------------------ ASM2 ARCHIVE UTILITY ---------------------------------
&SELECT OPTION ===>%OPT                                                        $
&
&   A$- QUEUE A DATASET TO BE ARCHIVED
&   R$- QUEUE A DATASET TO BE RELOADED FROM AN ARCHIVE TAPE
&   BLANK$- INQUIRE ABOUT ARCHIVED DATASET(S)
$
$COMPLETE DSN (OR PARTIAL DSN, WITHOUT QUOTES, FOR INQUIRY.)
$  NAME &===>%DSN                                                $
$
$FOR ARCHIVE REQUEST (OPTION A)
$   RETENTION  &===>%RET$  PERMANENT ARCHIVE &===>%P$ (Y OR N)
$   QUALIFIER  &===>%QUAL    $
$   COMMENT    &===>%CMT                           $
$
$FOR INQUIRY REQUEST (OPTION 'BLANK')
$   CYCLE      &===>%CYC$  (ONLY DATASETS ARCHIVED WITHIN LAST NNN DAYS)
$   COMMENT    &===>%C$    (ENTER 'Y' TO DISPLAY COMMENTS)
$
$
$
$
$
$
<ACTION>
 1 OPT     CHAR (1)  INIT(' ')     LIST   (A,R,' ',*) GENHELP(VT$AR)
                       CURSOR      RETURN (A,R,' ',?);
 2 OPT     CHAR (1)  INIT(' ')     LIST   ('A' ,  * )  KEY(SW1)
                     NOCURSOR      RETURN (' ' , '¬');
 3 OPT     CHAR (1)  INIT(' ')     LIST   (' ' ,  * )  KEY(SW2)
                     NOCURSOR      RETURN (' ' , '¬');
 4 OPT     CHAR (1)  INIT(' ')     LIST   ('R' ,  * )  KEY(SW3)
                     NOCURSOR      RETURN (' ' , '¬');
 5 DSN     CHAR (51) INIT(' ')     LIST   (' ' ,  * )
                     NOCURSOR      RETURN (' '      ); OPTIONAL
 6 DSN     CHAR (1)  INIT(' ')     LIST   (' ' ,  * )  KEY(SW8)
                     NOCURSOR      RETURN ('¬' , ' ');
 7 RET     CHAR (3)  INIT(' ')     LIST   (' ' ,  * )
                     NOCURSOR      RETURN (' '      ); OPTIONAL
 8 RET     CHAR (1)  INIT(' ')     LIST   (' ' ,  * )  KEY(SW4)
                     NOCURSOR      RETURN ('¬' , ' ');
 9 P       CHAR (1)  INIT('N')     LIST   ('Y' , 'N',  * )
                     NOCURSOR      RETURN ('Y' , 'N',  ? );
10 P       CHAR (1)  INIT('N')     LIST   ('Y' ,  * )  KEY(SW6)
                     NOCURSOR      RETURN (' ' , '¬');
11 QUAL    CHAR (8)  INIT(' ')     LIST   (' ' ,  * )
                     NOCURSOR      RETURN (' '      ); OPTIONAL
12 QUAL    CHAR (1)  INIT(' ')     LIST   (' ' ,  * )  KEY(SW5)
                     NOCURSOR      RETURN ('¬' , ' ');
13 CMT     CHAR (30) INIT(' ')     LIST   (' ' ,  * )
                     NOCURSOR      RETURN (' '      ); OPTIONAL
14 CMT     CHAR (1)  INIT(' ')     LIST   (' ' ,  * )  KEY(SW7)
                     NOCURSOR      RETURN ('¬' , ' ');
15 C       CHAR (1)  INIT('N')     LIST   ('Y' , 'N',  * )
                     NOCURSOR      RETURN ('Y' , 'N',  ? );
16 C       CHAR (1)  INIT('N')     LIST   ('Y' ,  * )  KEY(SWB)
                     NOCURSOR      RETURN (' ' , '¬');
17 CYC     CHAR (3)  INIT(' ')     LIST   (' ' ,  * )
                     NOCURSOR      RETURN (' '      ); OPTIONAL
18 CYC     CHAR (1)  INIT(' ')     LIST   (' ' ,  * )  KEY(SW9)
                     NOCURSOR      RETURN ('¬' , ' ');
<END>
./ ADD NAME=V$BK     0119-80014-80021-0729-00058-00061-00000-TYD9801 00
&------------------------ ASM2 BACKUP UTILITY ----------------------------------
&SELECT OPTION ===>%OPT                                                        $
&
&   B$- QUEUE A DATASET TO BE BACKED UP
&   R$- QUEUE A DATASET TO BE RELOADED FROM AN BACKUP TAPE
&   BLANK$- INQUIRE ABOUT BACKED UP DATASET(S)
$
$COMPLETE DSN (OR PARTIAL DSN, WITHOUT QUOTES, FOR INQUIRY)
$  NAME &===>%DSN                                                $
$
$FOR ARCHIVE REQUEST (OPTION B)
$   RETENTION  &===>%RET$
$   QUALIFIER  &===>%QUAL    $
$   COMMENT    &===>%CMT                           $
$
$FOR INQUIRY REQUEST (OPTION 'BLANK')
$   CYCLE      &===>%CYC$ (ONLY DATASETS BACKED UP WITHIN LAST NNN DAYS)
$   COMMENT    &===>%C$   (ENTER 'Y' TO DISPLAY COMMENTS)
$
$
$
$
$
$
<ACTION>
 1 OPT     CHAR (1)  INIT(' ')     LIST   (B,R,' ',*) GENHELP(VT$BK)
                       CURSOR      RETURN (B,R,' ',?);
 2 OPT     CHAR (1)  INIT(' ')     LIST   ('B' ,  * )  KEY(SW1)
                     NOCURSOR      RETURN (' ' , '¬');
 3 OPT     CHAR (1)  INIT(' ')     LIST   (' ' ,  * )  KEY(SW2)
                     NOCURSOR      RETURN (' ' , '¬');
 4 OPT     CHAR (1)  INIT(' ')     LIST   ('R' ,  * )  KEY(SW3)
                     NOCURSOR      RETURN (' ' , '¬');
 5 DSN     CHAR (51) INIT(' ')     LIST   (' ' ,  * )
                     NOCURSOR      RETURN (' '      ); OPTIONAL
 6 DSN     CHAR (1)  INIT(' ')     LIST   (' ' ,  * )  KEY(SW8)
                     NOCURSOR      RETURN ('¬' , ' ');
 7 RET     CHAR (3)  INIT(' ')     LIST   (' ' ,  * )
                     NOCURSOR      RETURN (' '      ); OPTIONAL
 8 RET     CHAR (1)  INIT(' ')     LIST   (' ' ,  * )  KEY(SW4)
                     NOCURSOR      RETURN ('¬' , ' ');
10 QUAL    CHAR (8)  INIT(' ')     LIST   (' ' ,  * )
                     NOCURSOR      RETURN (' '      ); OPTIONAL
11 QUAL    CHAR (1)  INIT(' ')     LIST   (' ' ,  * )  KEY(SW5)
                     NOCURSOR      RETURN ('¬' , ' ');
12 CMT     CHAR (30) INIT(' ')     LIST   (' ' ,  * )
                     NOCURSOR      RETURN (' '      ); OPTIONAL
13 CMT     CHAR (1)  INIT(' ')     LIST   (' ' ,  * )  KEY(SW7)
                     NOCURSOR      RETURN ('¬' , ' ');
14 C       CHAR (1)  INIT('N')     LIST   ('Y' , 'N',*)
                     NOCURSOR      RETURN ('Y' , 'N',?);
15 C       CHAR (1)  INIT('N')     LIST   ('Y' , 'N')  KEY(SWB)
                     NOCURSOR      RETURN (' ' , '¬');
16 CYC     CHAR (3)  INIT(' ')     LIST   (' ' ,  * )
                     NOCURSOR      RETURN (' '      ); OPTIONAL
17 CYC     CHAR (1)  INIT(' ')     LIST   (' ' ,  * )  KEY(SW9)
                     NOCURSOR      RETURN ('¬' , ' ');
<END>
./ ADD NAME=V$SM     0110-80014-80018-1638-00048-00060-00000-TYD9801 00
&---------------- ASM2 DISK SPACE MANAGEMENT UTILITY ---------------------------
&SELECT OPTION ===>%OPT                                                        $
&
&   BLANK$- DISPLAY DISK DATASET SPACE INFORMATION
&   U$- DISPLAY DISK DATASET USAGE INFORMATION
&   C$- COMPRESS DATASET WITHOUT RELEASING UNUSED SPACE
&   R$- RELEASE UNUSED SPACE IN DATASETS (COMPRESS OPTIONAL)
&   E$- RESET END-OF-FILE TO BEGINNING OF SEQUENTIAL DATASET
$
$TYPE OF NAME  &===>%T$     (L=INDEX LEVELS, BLANK=FULL DSN))
$   NAME       &===>%DSN                                         $
$
$OPTIONS FOR RELEASE/COMPRESS OPTION (OPTION 'R','C')
$   COMPRESS   &===>%C$     (FOR PDS ONLY, 'Y' OR 'N')
$   BACKUP     &===>%B$     (FOR COMPRESS ONLY, 'Y' OR 'N')
$   SAVE       &===>%S    $ (SAVE NNNNN UNUSED TRACKS)
<ACTION>
 1 OPT     CHAR (1)  INIT(' ')     LIST   (U,R,E,C,' ',*) GENHELP(VT$SM)
                       CURSOR      RETURN (U,R,E,C,' ',?);
 2 OPT     CHAR (1)  INIT(' ')     LIST   ('U' ,  * )  KEY(SW1)
                     NOCURSOR      RETURN ('¬' , ' ');
 3 OPT     CHAR (1)  INIT(' ')     LIST   ('U' ,  * )  KEY(SW2)
                     NOCURSOR      RETURN (' ' , '¬');
 4 OPT     CHAR (1)  INIT(' ')     LIST   ('R' ,  * )  KEY(SW7)
                     NOCURSOR      RETURN (' ' , '¬');
 5 OPT     CHAR (1)  INIT(' ')     LIST   ('E' ,  * )  KEY(SW8)
                     NOCURSOR      RETURN (' ' , '¬');
 6 OPT     CHAR (1)  INIT(' ')     LIST   ('C' ,  * )  KEY(SW9)
                     NOCURSOR      RETURN (' ' , '¬');
 7 DSN     CHAR (44) INIT(' ')     LIST   (' ' ,  * )
                     NOCURSOR      RETURN (' '      ); OPTIONAL
 8 DSN     CHAR (1)  INIT(' ')     LIST   (' ' ,  * )  KEY(SW3)
                     NOCURSOR      RETURN ('¬' , ' ');
 9 T       CHAR (1)  INIT(' ')     LIST   (' ' , 'L' ,  * )
                     NOCURSOR      RETURN (' ' , 'L' ,  ? ); OPTIONAL
10 T       CHAR (1)  INIT(' ')     LIST   (' ' ,  * )  KEY(SW4)
                     NOCURSOR      RETURN ('¬' , ' ');
11 T       CHAR (1)  INIT(' ')     LIST   ('L' ,  * )  KEY(SW5)
                     NOCURSOR      RETURN ('¬' , ' ');
12 C       CHAR (2)  INIT('Y')     LIST   ('N' ,  'Y' ,  * )
                     NOCURSOR      RETURN ('NO' , ' ' ,  ? ); REQUIRED
13 B       CHAR (2)  INIT('Y')     LIST   ('N' ,  'Y' ,  * )
                     NOCURSOR      RETURN ('NO' , ' ' ,  ? ); REQUIRED
14 S       CHAR (5)  INIT(' ')     LIST   (' ' ,  * )
                     NOCURSOR      RETURN (' '      ); OPTIONAL
15 S       CHAR (1)  INIT(' ')     LIST   (' ' ,  * )  KEY(SW6)
                     NOCURSOR      RETURN ('¬' , ' ');
<END>
./ ADD NAME=VAPRIOPT 0108-79288-80108-1411-00157-00144-00000-$CSG05  00
&-----------------------  SPF/TSO PRIMARY OPTION MENU  -------------------------
&SELECT OPTION ===>%OPT     $
&                                                           $USERID   -@ID
&   0 $SPF PARMS  - SPECIFY TERMINAL AND SPF PARAMETERS     $TIME     -@TIME
$                   1) TERMINAL  2) LOG/LIST  3) PF KEYS    $TERMINAL -@TERM
&   1 $BROWSE     - DISPLAY SOURCE DATA OR OUTPUT LISTINGS  $PFKEYS   -@KY
&   2 $EDIT       - CREATE OR CHANGE SOURCE DATA
&   3 $UTILITY    - PERFORM SPF UTILITY FUNCTIONS
$                   1) LIBRARY    3) MOVE/COPY   5) RESET      7) VTOC
$                   2) DATA SET   4) CATALOG     6) HARDCOPY   8) OUTLIST
&   4 $FOREGROUND - COMPILE, ASSEMBLE, OR LINK EDIT, OR DEBUG
&   5 $BACKGROUND - COMPILE, ASSEMBLE, OR LINK EDIT
&   6 $TSO        - ENTER TSO COMMAND OR CLIST
&   7 $TUTORIAL   - DISPLAY INFORMATION ABOUT SPF
&   A $ASM2       - ASM2 DISK STORAGE MANAGEMENT SYSTEM
$                   1) ARCHIVE  2) BACKUP    3) SPACE MANAGEMENT
&   P $PANVALET   - SPF/TSO/PANVALET INTERFACE
$                               2) CHANGE    4) DISPLAY  6) PRINT    8) RETRIEVE
$                   1) ADD      3) COPY      5) LISTDS   7) RENAME   9) STORE
&   J $JTIP       - PERFORM JTIP UTILITY FUNCTIONS
&   X $EXIT       - TERMINATE SPF USING LIST/LOG DEFAULTS
&
&
$PRESS&PF3$(END KEY) TO TERMINATE SPF$
&
<ACTION>
1 OPT CHAR(8) CURSOR GENHELP(TTUTOR)
 LIST    (0       ,0.1     ,0.2     ,0.3     ,
  1       ,
  2       ,
  3       ,3.1     ,3.2     ,3.3     ,3.4     ,3.5     ,3.6     ,3.7    ,
  3.8     ,3.9     ,
  4       ,4.1     ,4.2     ,4.3     ,4.4     ,4.5     ,4.6     ,4.7    ,
  4.8     ,
  5       ,5.1     ,5.2     ,5.3     ,5.4     ,5.5     ,5.6     ,5.7    ,
  5.8     ,
  6       ,
  7       ,
  8       ,8.0     ,8.1     ,8.2     ,8.3     ,8.4     ,8.5     ,8.6    ,
